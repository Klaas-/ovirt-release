[ovirt-@OVIRT_SLOT@-virtio-win-latest]
name=virtio-win builds roughly matching what will be shipped in upcoming RHEL
baseurl=http://fedorapeople.org/groups/virt/virtio-win/repo/latest
enabled=1
skip_if_unavailable=1
gpgcheck=0

[sbonazzo-python-ovsdbapp]
#fc28 is rawhide
name=Copr repo for python-ovsdbapp owned by sbonazzo
baseurl=https://copr-be.cloud.fedoraproject.org/results/sbonazzo/python-ovsdbapp/fedora-rawhide-$basearch/
type=rpm-md
skip_if_unavailable=True
gpgcheck=1
gpgkey=https://copr-be.cloud.fedoraproject.org/results/sbonazzo/python-ovsdbapp/pubkey.gpg
repo_gpgcheck=0
enabled=1
enabled_metadata=1

